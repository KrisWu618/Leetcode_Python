   "source": [
    "def factorial(n):\n",
    "    f = 1\n",
    "    while n > 0:\n",
    "        f *= n\n",
    "        n -= 1\n",
    "    return f"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "collapsed": true
   },
   "outputs": [],
   "source": [
    "def factorial_recursion(n):\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    else:\n",
    "        return n*factorial_recursion(n - 1)"
   ]
  },
